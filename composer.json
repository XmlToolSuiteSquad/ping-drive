{
    "name": "xmlsquad/ping-drive",
    "description": "Symfony Console command that reports its attempts at locating and reading the contents of a Google Drive folder or file",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Surgie Finesse",
            "email": "finesserus@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "xmlsquad/xml-authoring-library": "dev-master",
        "symfony/console": "^3.4",
        "symfony/yaml": "^3.4",
        "symfony/filesystem": "^3.4",
        "google/apiclient": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.1"
    },
    "autoload": {
        "psr-4": {
            "XmlSquad\\PingDrive\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "XmlSquad\\PingDrive\\Tests\\": "tests/"
        }
    },
    "bin": [
        "bin/ping-drive"
    ],
    "scripts": {
        "test": "phpunit"
    }
}
